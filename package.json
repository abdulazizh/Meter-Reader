{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "expo:dev": "cross-env EXPO_PUBLIC_DOMAIN=http://localhost:5000 npx expo start",
    "server:dev": "cross-env NODE_ENV=development tsx --env-file=.env server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node --env-file=.env scripts/build.cjs",
    "server:build": "npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist --alias:@shared=./shared",
    "server:prod": "cross-env NODE_ENV=production node server_dist/index.js",
    "build": "npm install && npm run server:build && npm run db:push",
    "start": "npm run server:prod",
    "db:push": "npx drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "test:server": "vitest run",
    "test:client": "jest"
  },
  "dependencies": {
    "@expo-google-fonts/cairo": "^0.4.2",
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@supabase/supabase-js": "^2.91.0",
    "@tanstack/react-query": "^5.90.7",
    "@types/express-session": "^1.18.2",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "esbuild": "^0.27.2",
    "expo": "~54.0.31",
    "expo-blur": "~15.0.8",
    "expo-camera": "^17.0.10",
    "expo-constants": "~18.0.9",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-location": "^19.0.8",
    "expo-media-library": "^18.2.1",
    "expo-sharing": "^14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "^16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "http-proxy-middleware": "^3.0.5",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tsx": "^4.20.6",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "24.10.0",
    "@types/react": "~19.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "prettier": "3.6.2",
    "react-test-renderer": "^19.2.4",
    "typescript": "~5.9.2",
    "vitest": "^4.0.18"
  },
  "private": true
}
